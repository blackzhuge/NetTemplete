{
  "$schema": "../schema.json",
  "id": "ai-ready",
  "name": "AI Ready",
  "description": "AI 对话应用配置，集成 MateChat",
  "isBuiltin": true,
  "tags": ["ai", "chat", "matechat"],
  "config": {
    "basic": {
      "projectName": "MyApp",
      "namespace": "MyApp"
    },
    "backend": {
      "architecture": "Simple",
      "orm": "SqlSugar",
      "database": "SQLite",
      "cache": "MemoryCache",
      "swagger": true,
      "jwtAuth": true
    },
    "frontend": {
      "uiLibrary": "MateChat",
      "routerMode": "Hash",
      "mockData": false
    }
  }
}
