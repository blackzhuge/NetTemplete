{
  "$schema": "../schema.json",
  "id": "startup",
  "name": "Startup",
  "description": "快速启动配置，Simple + SqlSugar + Naive UI",
  "isBuiltin": true,
  "tags": ["startup", "quick-start", "naive-ui"],
  "config": {
    "basic": {
      "projectName": "MyApp",
      "namespace": "MyApp"
    },
    "backend": {
      "architecture": "Simple",
      "orm": "SqlSugar",
      "database": "SQLite",
      "cache": "MemoryCache",
      "swagger": true,
      "jwtAuth": false
    },
    "frontend": {
      "uiLibrary": "NaiveUI",
      "routerMode": "Hash",
      "mockData": true
    }
  }
}
